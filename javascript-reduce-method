//This function accumulates the value of the newNumbers array, beginning with a specified value (10 in this case).
//It then logs it to the console. The purpose here is to practice using the reduce method with callback functions.

const newNumbers = [1, 3, 5, 7];

const newSum = newNumbers.reduce((accumulator, currentValue) => {
  return accumulator + currentValue;
  console.log('The value of accumulator: ' + accumulator);
  console.log('The value of currentValue: ' + currentValue);
}, 10);

console.log(newSum);
